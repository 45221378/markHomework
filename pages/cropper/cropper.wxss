.top{
  position: absolute;
  width:100%;
  top:10rpx;
  display: flex;
  flex-flow:wrap;
  z-index: 10;
  color:white;
  justify-content:space-around;
}
.hint{
  position: absolute;
  top:10rpx;
  width:100%;
  font-size: 33rpx;
  text-align: center;
  color:white;
  z-index: 10;
}
page{
  background:rgb(14, 13, 13);
}
view{
  font-size: 30rpx;
}
.bottom{
  position: absolute;
  width: 100%;
  bottom: 30rpx;
  display: flex;
  z-index: 10;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  left: 50%;
  transform: translateX(-50%);
}
.bottom .tips{
  font-size: 28rpx;
  text-align: center;
  color:white;
}
.cro-btn{
  z-index:2;
  height:96rpx;
  width:96rpx !important;
  margin:24rpx 0 0 0;
}
.input{
  display: flex;
  height: 50rpx;
  width:50%;
}
.input>.label{
  min-width:150rpx;
  font-size: 30rpx;
  height: 50rpx;
  line-height: 50rpx;
}
.input>input{
  margin-left:10rpx;
  text-align: center;
  max-width:160rpx;
  border:1px solid rgb(255, 255, 255);
  height: 50rpx;
  line-height: 50rpx;
  min-height: 50rpx;
  box-sizing: border-box;
}
.cutImg{
  z-index: 9;
  position: absolute;
  width: 100%;
  top: 0;
  left: 0;
}
.cutImg .allImg{
  height: 130rpx;
  padding-bottom: 10rpx;

}
.cutImg .allImg scroll-view{
  height: 100%;
}
.cutImg .dotImg{
  height:100%;
  width: 100rpx;
  margin-right: 20rpx;
}