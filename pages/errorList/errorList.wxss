/* pages/errorList/errorList.wxss */
/* 按章节页面的css */
.errorList .download {
  font-size: 28rpx;
  color: #333;
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 100rpx;
  width: 100%;
  padding: 0 40rpx;
  background-color: #f9f9f9;
}

.errorList .download .download-img {
  height: 26rpx;
  width: 26rpx;
  margin: 0 4rpx 0 0;
}

.errorList .download .download-btn {
  width: 176rpx;
  height: 50rpx;
  background-color: #2896fe;
  line-height: 50rpx;
  text-align: center;
  font-size: 28rpx;
  color: #fff;
  border-radius: 24rpx;
}

.errorList .check-title {
  height: 100%;
}

.errorList .check-title .choose-title {
  font-size: 30rpx;
  color: #333;
  padding: 20rpx 0 0 40rpx;

}

.errorList .check-title .ul {
  background-color: #fff;
}

.errorList .check-title .ul .li {
  padding: 48rpx 0rpx 0rpx 0rpx;
  margin: 0 48rpx;
  border-bottom: 1rpx solid #eee;
}

.mgr0 {
  margin: 0 !important;
}

.magl48 {
  margin: 0 0 40rpx 58rpx !important;
}

.errorList .check-title .ul .li:last-child {
  border: none !important;
}

.errorList .check-title .ul .li-left {
  font-size: 32rpx;
  color: #333;
  border-radius: 4rpx;
  height: 44rpx;
  line-height: 44rpx;
  margin: 0 30rpx 40rpx 0rpx;
}


.errorList .check-title .ul .li-content {
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  justify-content: flex-start;
}

.errorList .check-title .ul .li-content .li-content-dot {
  width: 25%;
  height: 116rpx;
  margin-bottom: 60rpx;
}

.errorList .check-title .ul .li-content .li-content-dot .dot-left {
  height: 100%;
  width: 100%;
}


.li-content-dot .dot-left .bio {
  font-size: 26rpx;
  color: #333;
  margin-top: 20rpx;
}

.errorList .check-title .ul .li-content .li-content-dot .dot-right {
  background-color: #f7fbff;
  float: right;
  height: 100%;
  width: 138rpx;
  text-align: center;
}

.li-content-dot .dot-right .nums {
  font-size: 30rpx;
  color: #3a9efd;
  margin: 8rpx 0 0 0;

}

.li-content-dot .dot-right .recall {
  font-size: 20rpx;
  color: #666;
}

