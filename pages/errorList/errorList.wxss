/* 扫一扫按钮样式 */
.area {
  width: 100vw;
  height: calc(100vh - 100rpx);
  position: fixed;
  bottom: 0;
  z-index: 9999;
  border-top: 1rpx solid #f9f9f9;
}

.view {
  height: 200rpx;
  width: 200rpx;
  z-index: 999;
}

.errorList .scan {
  height: 200rpx;
  width: 200rpx;
  border-radius: 50%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.errorList .scan image {
  height: 60rpx;
  width: 60rpx;
}

.errorList .scan text {
  font-size: 30rpx;
  color: #fff;
  margin-top: 14rpx;
}

.zfx {
  width: 100rpx;
  height: 100rpx;
  background-color: #2896fe;
}